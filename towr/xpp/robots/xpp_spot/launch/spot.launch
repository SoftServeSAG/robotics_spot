<?xml version="1.0"?>
<launch>
 
  <!-- Upload URDF file to ros parameter server for rviz to find  -->
  <param name="spot_description" command="$(find xacro)/xacro --inorder '$(find spot_description)/urdf/spot.urdf.xacro'"/>
  
  <!-- Converts Cartesian state to joint state and publish TFs to rviz  --> 
  <node name="urdf_visualizer_spot4" pkg="xpp_spot" type="urdf_visualizer_spot4" output="screen"/>
     
</launch>
